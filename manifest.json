{
  "manifest_version": 2,
  "name": "Tab Refresh",
  "version": "1.0",
  "description": "Refreshes a browser tab for a set time interval",
  "permissions": [
    "tabs",
    "activeTab",
    "background"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
